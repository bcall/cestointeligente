<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
 
	<bean id="ConstansBean" class="ar.com.itr.cestointeligente.Constants">
		<!-- java.util.Map -->
		<property name="msgColor">
			<map>
				<entry key="1" value="alert-warning" />
				<entry key="2" value="alert-info" />
				<entry key="3" value="alert-info" />
				<entry key="4" value="alert-warning" />
				<entry key="5" value="alert-success" />
			</map>
		</property>
		<property name="msgIcon">
			<map>
			    <entry key="1" value="fa fa-folder-open-o" />
				<entry key="2" value="fa fa-level-up" />
				<entry key="3" value="fa fa-level-down" />
				<entry key="4" value="fa fa-history" />
				<entry key="5" value="fa fa-check-circle" />
			</map>
		</property>
		<property name="msgStatus">
			<map>
			    <entry key="1" value="ABIERTA" />
				<entry key="2" value="ARRIBA" />
				<entry key="3" value="ABAJO" />
				<entry key="4" value="INICIO" />
				<entry key="5" value="VIVO" />
			</map>
		</property>
	</bean>
</beans>